{
  "include": [
    "src",
    "tests",
    "*.ts"
, "test"  ],
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": [
      "vitest/globals"
    ],
    "paths": {
      "@/":[
        "src"
      ],
      "@*":[
        "src/*"
      ],
      "@/*": [
        "src/*"
      ],
    }
  }
}
